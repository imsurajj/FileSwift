# Production environment settings - used during build and in production

# Skip database connection during build
SKIP_DB_CONNECT=true

# Disable features that cause build issues
PRISMA_CLIENT_NO_SCHEMA_VALIDATION=true

# Auth.js Environment Variables
# Note: NEXTAUTH_URL is set automatically by Vercel

# Database connection - empty file for build process
# This will be overridden by environment variables in Vercel
DATABASE_URL="file:./prisma/database.db" 